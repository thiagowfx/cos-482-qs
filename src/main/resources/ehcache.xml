<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <!--
      This is a default configuration, it is re-configured by the CacheConfiguration Spring Bean, using the
      properties from the resources/config/*.yml files.
    -->

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />

    <cache name="br.ufrj.cos482.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos482.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos482.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos482.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="br.ufrj.cos482.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>
    
    <cache name="br.ufrj.cos482.domain.Usuario"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="br.ufrj.cos482.domain.Aluno"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="br.ufrj.cos482.domain.Aluno.orientadors"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="br.ufrj.cos482.domain.SecretarioAcademico"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="br.ufrj.cos482.domain.AlunoMestrado"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="br.ufrj.cos482.domain.AlunoDoutorado"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="br.ufrj.cos482.domain.Professor"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="br.ufrj.cos482.domain.DocumentoIdentificacao"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <cache name="br.ufrj.cos482.domain.DocumentoSistema"
                               timeToLiveSeconds="3600">
                        </cache>
                        
    <!-- jhipster-needle-ehcache-add-entry -->
</ehcache>
